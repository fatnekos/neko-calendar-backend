{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": [
      "esnext"
    ],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "noImplicitAny": true,
    "plugins": [
      {
        "name": "deno_ls_plugin"
      }
    ],
    "pretty": true,
    "resolveJsonModule": true,
    "target": "esnext"
  },
  "include": [
    "./**/*.ts"
  ]
}